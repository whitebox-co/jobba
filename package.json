{
  "name": "jobba",
  "version": "0.6.1",
  "description": "Jobba the Hub",
  "main": "dist/lib/index.js",
  "scripts": {
    "build": "tsc && [ -d ./dist/bin ] && chmod -f +x ./dist/bin/* || exit 0",
    "clean": "rm -rf ./dist",
    "lint": "tslint -t stylish './src/**/*.ts' || exit 0",
    "lint:verbose": "tslint -t codeFrame './src/**/*.ts' || exit 0",
    "postinstall": "npm run build",
    "watch": "tsc -w"
  },
  "keywords": [
    "job",
    "hub"
  ],
  "author": "Ross Hadden",
  "license": "ISC",
  "dependencies": {
    "bull": "^3.5.2",
    "bull-arena": "git+https://github.com/whitebox-co/arena.git#dev",
    "chalk": "^2.4.2",
    "joi": "^13.7.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-express": "^1.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "yawk": "git+https://github.com/rosshadden/yawk.git"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/bull": "^3.5.1",
    "@types/joi": "^13.6.3",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^5.0.2",
    "@types/koa-router": "^7.0.37",
    "@types/lodash": "^4.14.119",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.18",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  }
}
